<!DOCTYPE html>
<html>
<%= render 'layouts/header' %>
<body>
  <% if flash[:error] %>
    <div id="error_explanation" xmlns="http://www.w3.org/1999/html">
      <h2><%= flash[:error] %></h2>
    </div>
  <% end %>

  <% if flash[:notice] %>
    <div id="notice">
      <h2><%= flash[:notice] %></h2>
    </div>
  <% end %>

  <div class="container">
    <%= yield %>
  </div>
</body>

<%= render 'layouts/footer' %>
<%= debug(params) if Rails.env.development?%>

</html>



